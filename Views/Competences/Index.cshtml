@model IEnumerable<WebApplication.Models.Competence>

@{
    ViewBag.Title = "Компетенции";
}

<h2>Список компетенций</h2>

<p>
    @Html.ActionLink("Добавить компетенцию", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Изменить", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Детали", "Details", new { id=item.Id }) |
            @Html.ActionLink("Удаление", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
